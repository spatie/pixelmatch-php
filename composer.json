{
    "name": "spatie/pixelmatch-php",
    "description": "A pixel-level image comparison package for PHP",
    "keywords": [
        "spatie",
        "pixelmatch-php",
        "pixelmatch"
    ],
    "homepage": "https://github.com/spatie/pixelmatch-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Vanpachtenbeke",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^2.3",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PixelMatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PixelMatch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
